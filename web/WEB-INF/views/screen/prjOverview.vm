<div class="content">
    <div class="leftDiv">
        <ul class="picList" id="picList">
            #foreach($pic in $picList)
                <li  #if($velocityCount == 1)class="active"#end>
                    <img src="$!{service_name}$!{pic.PIC_URL}" />
                    <div class="bottom-desc">
                        <p class="desc-title">$!{pic.PIC_DESC_TITLE}</p>
                        <p class="desc-content">$!{pic.PIC_DESC_CONTENT}</p>
                    </div>
                </li>
            #end
        </ul>
        <span class="pre" onclick="pre()"><img src="$!{service_name}/resources/images/icon/pre.png"/> </span>
        <span class="next" onclick="next()"><img src="$!{service_name}/resources/images/icon/next.png"/> </span>
    </div>
    <div class="rightDiv">
        <ul class="overviewList">
            #foreach($module in $modulesList)
                <li id="$!{catalog.ID}">
                    <div class="overviewItem">
                        <div class="imgDiv">
                            <img src="$!{service_name}$!{module.MODULE_IMG}"/>
                        </div>

                        <p>$!{module.MODULE_NAME}</p>
                    </div>
                </li>
            #end
        </ul>
    </div>
</div>