<link rel="stylesheet" href="$!{service_name}/resources/fontAwesome/css/font-awesome.css" type="text/css">
<!--[if lt IE 10]>
<script type="text/javascript" src="$!{service_name}/resources/pie/PIE.js"></script>
<![endif]-->
<script language="javascript">
    $(function() {
        if (window.PIE) {
            $('.content .leftDiv span').each(function() {
                PIE.attach(this);
            });
        }
    });
</script>
