<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
    <settings cacheModelsEnabled="true" enhancementEnabled="true"
              lazyLoadingEnabled="true" errorTracingEnabled="true"
              maxRequests="512" maxSessions="128" maxTransactions="64"
              useStatementNamespaces="true" />

    <typeAlias alias="dto" type="com.freemind.cube.common.datastructure.impl.BaseDto"/>
    <typeAlias alias="entryMap" type="com.freemind.cube.common.vo.EntryMap"/>

    <sqlMap resource="config/db/sqlmap/common.xml"/>
    <sqlMap resource="config/db/sqlmap/system.xml"/>

</sqlMapConfig>
